	<Image x:Key="ModioIcon"
	    Width="16"
	    Height="16"
	    Source="{SvgImage /Assets/Icons/modio_Blue.svg}" />
	<Image x:Key="NexusModsIcon"
	    Width="16"
	    Height="16"
	    Source="{SvgImage /Assets/Icons/nexuslogo.svg}" />
</ResourceDictionary>
