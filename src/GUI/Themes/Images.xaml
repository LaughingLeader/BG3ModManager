		x:Key="LargeFileIcon"
		Width="36"
		Height="36"
		x:Shared="false"
		Source="{svgc:SvgImage Source=/Resources/Icons/CurrentFile_16x.svg,
		                       AppName=BG3ModManager}" />
	<Image
		x:Key="LargeFolderIcon"
		Width="36"
		Height="36"
		x:Shared="false"
		Source="{svgc:SvgImage Source=/Resources/Icons/Folder_16x.svg,
		                       AppName=BG3ModManager}" />
	<Image
		x:Key="ListViewIcon"
		Width="16"
		Height="16"
		x:Shared="false"
		Source="{svgc:SvgImage Source=/Resources/Icons/ListView_16x.svg,
		                       AppName=BG3ModManager}" />
	<Image
		x:Key="CardViewIcon"
		Width="16"
		Height="16"
		x:Shared="false"
		Source="{svgc:SvgImage Source=/Resources/Icons/ViewPortrait_16x.svg,
		                       AppName=BG3ModManager}" />
	<svgc:SvgBitmap
		x:Key="HelpIcon"
		Width="16"
		Height="16"
		x:Shared="false"
		AppName="BG3ModManager"
		UriSource="/Resources/Icons/StatusHelp_16x.svg" />
</ResourceDictionary>