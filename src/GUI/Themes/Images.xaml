	<!--  Overrides the default context menu open animation to remove flickering.  -->
	<PopupAnimation x:Key="{x:Static SystemParameters.MenuPopupAnimationKey}">None</PopupAnimation>
	<!--  x:Shared is required so prevent the next MenuItem from unsetting the previous icon.  -->
	<Image
		x:Key="CopyToClipboardIcon"
		Width="16"
		Height="16"
		x:Shared="false"
		Source="{StaticResource CopyToClipboardImage}" />
	<Image
		x:Key="OpenFolderIcon"
		Width="16"
		Height="16"
		x:Shared="false"
		Source="{StaticResource OpenFolderImage}" />
	<Image
		x:Key="OpenFileIcon"
		Width="16"
		Height="16"
		x:Shared="false"
		Source="{StaticResource OpenFileImage}" />
	<Image
		x:Key="OpenInBrowserIcon"
		Width="16"
		Height="16"
		x:Shared="false"
		Source="{StaticResource OpenInBrowserImage}" />
	<Image
		x:Key="ModDeveloperIcon"
		Width="16"
		Height="16"
		x:Shared="false"
		Source="{StaticResource OsirisImage}" />
	<Image
		x:Key="ModPropertiesIcon"
		Width="16"
		Height="16"
		x:Shared="false"
		Source="{StaticResource ConfigurationFileImage}" />
